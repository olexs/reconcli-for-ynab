{
  "name": "reconcli-for-ynab",
  "version": "1.0.1",
  "description": "ReconCLI for YNAB - a CLI for quickly reconciling YNAB accounts",
  "keywords": [
    "ynab",
    "cli",
    "reconcile",
    "reconcli"
  ],
  "author": "Olexandr Savchuk",
  "license": "MIT",
  "homepage": "https://github.com/olexs/reconcli-for-ynab",
  "repository": "github:olexs/reconcli-for-ynab",
  "bugs": {
    "url": "https://github.com/olexs/reconcli-for-ynab/issues"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "bin": {
    "reconcli-for-ynab": "./dist/index.js"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc -p .",
    "run:local": "npm run build && sudo npm i -g && ynab-reconcile",
    "test": "jest",
    "lint": "eslint --ext .ts ./src"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "commander": "^9.2.0",
    "ynab": "^1.28.0"
  },
  "engines": {
    "node": ">=14"
  }
}
